<launch>
  <env name="GAZEBO_MODEL_PATH" value="$(find oto_sensor)/models"/>
  <arg name="x_pos" default="-50.5"/>
  <arg name="y_pos" default="-62.5"/>
  <arg name="yaw" default="0"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find oto_sensor)/worlds/harita.world" />
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find kasva_description)/urdf/kasva.urdf.xacro" />
  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model kasva -x $(arg x_pos) -y $(arg y_pos) -Y $(arg yaw) -param robot_description" />
</launch>


  